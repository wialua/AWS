-- Client-side patch for SimplePath
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local SimplePath = require(ReplicatedStorage:WaitForChild("OOP"):WaitForChild("SimplePath"))

-- Patch __index to avoid errors
local mt = getmetatable(SimplePath)
if mt and mt.__index then
    local oldIndex = mt.__index
    mt.__index = function(self, key)
        local ok, res = pcall(function()
            return oldIndex(self, key)
        end)
        return ok and res or nil
    end
end

-- Patch distance functions to avoid nil comparisons
for key, value in pairs(SimplePath) do
    if type(value) == "function" then
        SimplePath[key] = function(...)
            local args = {...}
            -- Replace nil vectors/positions with Vector3.zero
            for i = 1, #args do
                if typeof(args[i]) == "Vector3" and args[i] == nil then
                    args[i] = Vector3.new(0,0,0)
                end
            end
            local ok, res = pcall(value, ...)
            return ok and res or math.huge
        end
    end
end


