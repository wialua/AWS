-- Auto Tower Toggle
local AutoTowerToggle1 = TowerSection1:AddToggle("AutoTower", {
    Title = "Auto Tower (250 keys)",
    Description = "Will auto complete the tower doing 250 keys each time. Temporarily disables Auto Fight/Click.",
    Default = false
})

AutoTowerToggle1:OnChanged(function()
    if AutoTowerToggle1.Value then
        spawn(function()
            local player = game.Players.LocalPlayer
            local togglesDisabled = false
            local wasAutoFightOn = false
            local wasAutoClickOn = false

            while AutoTowerToggle1.Value do
                local inTower, _ = CheckTowerStatus()

                local keyObject = player.PlayerGui.GameUI.Menus.Inventory.Display.Items.MainFrame.ScrollingFrame.KeysStorage.Objects:FindFirstChild("Underworld Tower Key/1")
                local currentKeyAmount = 0
                if keyObject and keyObject:FindFirstChild("ButtonDisplay") and keyObject.ButtonDisplay:FindFirstChild("Amt") then
                    currentKeyAmount = parseTowerKeyAmount(keyObject.ButtonDisplay.Amt.Text)
                end

                local hasKeys = currentKeyAmount > 0

                -- Disable other toggles if keys present
                if hasKeys and not togglesDisabled then
                    wasAutoFightOn = AutoFightToggle and AutoFightToggle.Value or false
                    wasAutoClickOn = AutoClickToggle and AutoClickToggle.Value or false
                    if wasAutoFightOn then AutoFightToggle:SetValue(false) end
                    if wasAutoClickOn then AutoClickToggle:SetValue(false) end
                    togglesDisabled = true
                    wait(1)
                elseif not hasKeys and togglesDisabled and not inTower then
                    if wasAutoFightOn and AutoFightToggle then AutoFightToggle:SetValue(true) end
                    if wasAutoClickOn and AutoClickToggle then AutoClickToggle:SetValue(true) end
                    togglesDisabled = false
                end

                if not inTower and hasKeys then
                    pcall(function()
                        local ArmWrestleService = Knit.GetService("ArmWrestleService")
                        ArmWrestleService:RequestEndFight("SkipEnd")
                    end)
                    wait(0.5)
                    pcall(function()
                        game:GetService("ReplicatedStorage").Packages.Knit.Services.TowerService.RF.EnterTower:InvokeServer("UnderworldTower", true)
                    end)
                end

                wait(hasKeys and 1 or 5)
            end

            -- Re-enable toggles when manually turned off
            if togglesDisabled then
                wait(1)
                local inTower, _ = CheckTowerStatus()
                if not inTower then
                    if wasAutoFightOn and AutoFightToggle then AutoFightToggle:SetValue(true) end
                    if wasAutoClickOn and AutoClickToggle then AutoClickToggle:SetValue(true) end
                end
            end
        end)
    end
end)

local CupMinigameData = require(ReplicatedStorage.Data.Halloween.CupMinigame)
local CupMinigameController = Knit.GetController("CupMinigameController")
local CustomReplicaController = Knit.GetController("CustomReplicaController")

local MinigameSection = Tabs.Minigame:AddSection("Cup Minigame")

local CupMinigameToggle = MinigameSection:AddToggle("CupMinigameToggle", {
    Title = "100% Cup Minigame (Auto)",
    Description = "Automatically plays the cup minigame, always selects the right cup, and claims rewards every 10 keys.",
    Default = false
})

local CupParagraph = MinigameSection:Paragraph("CupParagraph", {
    Title = "Shoutout",
    Content = "A hugeeeeeeeeeeee thank you to a friend of mine who lent me over 100 cup keys to experiment with. I kept on wasting the keys with silly mistakes, but he kept giving me more and more keys/chances. Im not going to mention his name unless he wants me to, but thank you so much for the help!"
})

local currentHighlight
local startingKeys = 0
local nextClaimThreshold = 0

local function highlightCup(cup)
    if currentHighlight then
        currentHighlight:Destroy()
        currentHighlight = nil
    end
    local highlight = Instance.new("Highlight")
    highlight.FillColor = Color3.new(0, 1, 0)
    highlight.OutlineColor = Color3.new(1, 1, 0)
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Adornee = cup
    highlight.Parent = cup
    currentHighlight = highlight
end

local oldEndRound = CupMinigameController.EndRound
CupMinigameController.EndRound = function(...)
    if currentHighlight then
        currentHighlight:Destroy()
        currentHighlight = nil
    end
    return oldEndRound(...)
end

local function getCupKeyAmount()
    local replica = CustomReplicaController:GetReplica()
    if replica and replica.Data and replica.Data.Items and replica.Data.Items.ItemInventory and replica.Data.Items.ItemInventory.Keys then
        local keys = replica.Data.Items.ItemInventory.Keys
        return (keys["Cup Key"] and keys["Cup Key"]["1"]) or (keys["CupKey"] and keys["CupKey"]["1"]) or 0
    end
    return 0
end

local function fireCupButtonWhenVisible(billboard)
    local button = billboard:FindFirstChild("Button")
    if not button then return false end
    while not billboard.Enabled or not button.Visible do
        task.wait(0.1)
    end
    for _, conn in ipairs(getconnections(button.MouseButton1Click)) do
        conn:Fire()
    end
    return true
end

local function fireWinProceedButton()
    local LocalPlayer = Players.LocalPlayer
    local CupWin = LocalPlayer.PlayerGui.GameUI.Menus.CupWin
    local proceedButton = CupWin.Actions.Proceed.Button
    while not proceedButton.Visible do
        task.wait(0.1)
    end
    for _, conn in ipairs(getconnections(proceedButton.MouseButton1Click)) do
        conn:Fire()
    end
end

local function fireWinCancelButton()
    local LocalPlayer = Players.LocalPlayer
    local CupWin = LocalPlayer.PlayerGui.GameUI.Menus.CupWin
    local cancelButton = CupWin.Actions.Cancel.Button
    while not cancelButton.Visible do
        task.wait(0.1)
    end
    for _, conn in ipairs(getconnections(cancelButton.MouseButton1Click)) do
        conn:Fire()
    end
end

local function startCupGameIfInactive()
    if not CupMinigameController.Stand then
        local LocalPlayer = game:GetService("Players").LocalPlayer
        local PlayNowButton = LocalPlayer.PlayerGui.GameUI.Menus.CupEntrance.PlayNow.Button
        if PlayNowButton and PlayNowButton.Visible then
            for _, conn in ipairs(getconnections(PlayNowButton.MouseButton1Click)) do
                conn:Fire()
            end
            task.wait(1)
        end
    end
end
    
CupMinigameToggle:OnChanged(function()
    if CupMinigameToggle.Value then
        -- Initialize starting keys and claim threshold
        startingKeys = getCupKeyAmount()
        nextClaimThreshold = startingKeys - 10
        
        -- Make the round go instantly
        CupMinigameData.Config.TimeMultiplier = 0.01
        CupMinigameData.Config.StartSpeed = 99999
        CupMinigameData.Config.SpeedPerRound = 99999
        CupMinigameData.Config.MaxSpeed = 99999

        spawn(function()
            local canContinue = true
            
            while CupMinigameToggle.Value do
                local cupKeyAmount = getCupKeyAmount()
                
                -- Check if we've hit a multiple of 10 keys
                if cupKeyAmount % 10 == 0 and cupKeyAmount > 0 and cupKeyAmount < startingKeys then
                    canContinue = false
                    task.wait(3)
                    fireWinCancelButton()
                    task.wait(3)
                    startingKeys = cupKeyAmount
                    canContinue = true
                end

                -- Stop if completely out of keys
                if cupKeyAmount == 0 then
                    fireWinCancelButton()
                    task.wait(3)
                end

                if canContinue and not CupMinigameController.Stand and cupKeyAmount > 0 then
                    startCupGameIfInactive()
                    task.wait(1)
                end

                -- If in game, play the round
                if canContinue and CupMinigameController.Stand then
                    local highlightedCup = nil
                    while not highlightedCup and CupMinigameToggle.Value do
                        for _, cup in pairs(CupMinigameController.Stand.Cups:GetChildren()) do
                            local ball = CupMinigameController.Stand.Balls:FindFirstChild(cup.Name)
                            if ball and ball.PrimaryPart and ball.PrimaryPart.Transparency == 0 then
                                highlightCup(cup)
                                highlightedCup = cup
                                break
                            end
                        end
                        task.wait(0.1)
                    end

                    task.wait(0.5)

                    local billboard = CupMinigameController.CupBillboards[highlightedCup]
                    if billboard then
                        fireCupButtonWhenVisible(billboard)
                    else
                    end

                    task.wait(1)
                    fireWinProceedButton()
                    task.wait(2)
                else
                    -- Not in game or paused, wait 3 seconds before checking again
                    task.wait(3)
                end
            end
        end)
    end
end)

-- Hand the library over to our managers
SaveManager:SetLibrary(Library)
InterfaceManager:SetLibrary(Library)
SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes{}
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")


InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)
Window:SelectTab(1)
SaveManager:LoadAutoloadConfig()

